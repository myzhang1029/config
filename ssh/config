Host *.git*.com
    ControlMaster auto
    ControlPath ~/.ssh/conn-%r@%h:%p
    ControlPersist 120s

Host e.coding.net
    ControlMaster auto
    ControlPath ~/.ssh/conn-%r@%h:%p
    ControlPersist 120s
    IdentityFile ~/.ssh/id_rsa

Host *
    ControlMaster auto
    ControlPath ~/.ssh/conn-%r@%h:%p
    ControlPersist 4h
    ForwardAgent yes
    Compression yes
    ServerAliveInterval 30
    ServerAliveCountMax 120
    IdentityFile ~/.ssh/id_ed25519

